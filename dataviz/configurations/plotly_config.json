        "currentvalue": {
          "font": {"size": 20},
          "prefix": "Ordering: ",
          "visible": true,
          "xanchor": "left"
        },
        "transition": {
          "duration": 300,
          "easing": "cubic-in-out"
        },
        "pad": {
          "b": 10,
          "t": 80
        },
        "len": 0.5,
        "x": 0.25,
        "y": 0,
        "steps": [
          {
            "method": "animate",
            "label": "nonmagnetic",
            "args": [
              [
                "nonmagnetic"
              ],
              {
                "frame": {
                  "duration": 600,
                  "redraw": false
                },
                "mode": "immediate"
              }
            ]
          },
          {
            "method": "animate",
            "label": "ferromagnetic",
            "args": [
              [
                "ferromagnetic"
              ],
              {
                "frame": {
                "duration": 600,
                "redraw": false
              },
                "mode": "immediate"
              }
            ]
          },
          {
            "method": "animate",
            "label": "g-type",
            "args": [
              [
                "g-type"
              ],
              {
                "frame": {
                  "duration": 600,
                  "redraw": false
                },
                "mode": "immediate"
              }
            ]
          }
        ]
      }]
    }
  }
