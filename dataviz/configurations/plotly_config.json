{
  "figure": {
    "data": [],
    "frames": [],
    "layout": {
      "autosize": false,
      "width": 900,
      "height": 800,
      "title": "Density of states of BCC Iron",
      "xaxis": {
        "range": [-13, 10],
        "title": "Energy (eV)",
        "titlefont": {
          "size": 18
        },
        "showgrid": false,
        "showline": true,
        "mirror": true
      },
      "yaxis": {
        "title": "DOS (states/eV)",
